<header class="main__header">
	<h1>
		<span class="main__header__icon">
			<i class="fa-duotone fa-tools"></i>
		</span>
		<span class="main__header__label">Toolboard</span>
	</h1>
</header>

<div class="main__content">
	<section class="container">
		<div>
			<ul>
				<% for(let principe of principes) { %>
					<li>
						<article class="card">
							<header class="card__header">
								<h3><%= principe.titel %></h3>
							</header>

							<div class="card__content">
								<%- principe.beschrijving.html %>
							</div>
						</article>

					</li>
				<% } %>
			</ul>
		</div>

	</section>
</div>